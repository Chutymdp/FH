  </template>\r\n        </v-stepper-header>\r\n\r\n        <v-stepper-items>\r\n          \r\n          <v-stepper-content v-for=\"(paso,n) in pasos\" :key=\"`${n}-content`\" :step=\"n\">\r\n            <v-card class=\"mb-12\" color=\"white\" height=\"auto\">\r\n              <h5\r\n                class=\"text-center\"\r\n                style=\"font-family: 'Montserrat', sans-serif;\"\r\n              >{{ paso.titulo }}</h5>\r\n\r\n              <!--------Formulario o Contenido-------->\r\n\r\n              <component :is=\"paso.name\" :key=\"paso.name\"></component>\r\n\r\n              <!--------Final del Formulario o Contenido-------->\r\n            </v-card>\r\n\r\n            <v-btn\r\n              color=\"red\"\r\n              class=\"white--text\"\r\n              @click=\"backStep(n)\"\r\n              style=\"margin-right:10px\"\r\n            >Regresar</v-btn>\r\n            <v-btn color=\"primary\" @click=\"nextStep(n)\" style=\"margin-right:10px\">Siguiente</v-btn>\r\n          </v-stepper-content>\r\n        </v-stepper-items>\r\n      </v-stepper>\r\n    </div>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport Form_DatosPersonales from \"@/components/formularios/Form_DatosPersonales.vue\";\r\nimport Form_Ubicacion from \"@/components/formularios/Form_Ubicacion.vue\";\r\nimport Form_RedesSociales from \"@/components/formularios/Form_RedesSociales.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    \"Form_DatosPersonales\": Form_DatosPersonales,\r\n    \"Form_Ubicacion\": Form_Ubicacion,\r\n    \"Form_RedesSociales\":Form_RedesSociales\r\n  },\r\n  data() {\r\n    return {\r\n      e1: 0,\r\n      steps: 9,\r\n      pasos: [\r\n        {\r\n          titulo: \"Datos Personales\",\r\n          name: \"Form_DatosPersonales\"\r\n        },\r\n        {\r\n          titulo: \"Datos de Ubicación\",\r\n          name: \"Form_Ubicacion\"\r\n        },\r\n        {\r\n          titulo: \"Redes Sociales\",\r\n          name:\"Form_RedesSociales\"\r\n        },\r\n        {\r\n          titulo: \"Perfil Profesional\"\r\n        },\r\n        {\r\n          titulo: \"Idiomas\"\r\n        },\r\n        {\r\n          titulo: \"Historia Laboral\"\r\n        },\r\n        {\r\n          titulo: \"Formación\"\r\n        },\r\n        {\r\n          titulo: \"Cursos\"\r\n        },\r\n        {\r\n          titulo: \"Competencias\"\r\n        },\r\n        {\r\n          titulo: \"Vista Previa\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n\r\n  watch: {\r\n    steps(val) {\r\n      if (this.e1 > val) {\r\n        this.e1 = val;\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    nextStep(n) {\r\n      if (n === this.steps) {\r\n        this.e1 = 0;\r\n      } else {\r\n        this.e1 = n + 1;\r\n      }\r\n    },\r\n    backStep(n) {\r\n      //Termina el registro\r\n      if (n === this.steps) {\r\n        this.e1 = 0;\r\n      } else {\r\n        this.e1 = n - 1;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}