red\r\n                @input=\"$v.name.$touch()\"\r\n                @blur=\"$v.name.$touch()\"\r\n              ></v-text-field>\r\n            </v-col>\r\n          </v-row>\r\n        </form>\r\n      </v-col>\r\n    </v-row>\r\n    <!-------Botón para agregar------->\r\n    <v-row>\r\n      <v-col cols=\"12\" md=\"12\" justify-self=\"center\">\r\n        <v-btn class=\"ma-2\" color=\"teal\" dark @click=\"agregar()\">\r\n          <v-icon dark left>fas fa-plus</v-icon>Agregar\r\n        </v-btn>\r\n        <v-btn v-show=\"c >= 2\" class=\"ma-2\" color=\"pink darken-1\" dark @click=\"eliminar()\">\r\n          <v-icon dark left>fas fa-minus</v-icon>Eliminar\r\n        </v-btn>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row class=\"pa-2\" justify=\"center\">\r\n      <v-col class=\"d-flex pa-4\" cols=\"12\" sm=\"6\" md=\"12\">\r\n        <p style=\"font-family: 'Jomolhari', serif;\">\r\n          Lorem ipsum dolor sit amet consectetur adipisicing elit.\r\n          Reiciendis tempora alias aliquid quae beatae accusamus,\r\n          sequi culpa labore ullam praesentium rem. Laborum sit esse\r\n          eveniet aperiam illum. Sapiente, pariatur vel?\r\n        </p>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row>\r\n      <v-btn\r\n        color=\"red\"\r\n        class=\"white--text\"\r\n        @click=\"RegistrarCV()\"\r\n        style=\"margin-right:10px\"\r\n      >Regresar</v-btn>\r\n      <v-btn color=\"primary\" @click=\"agregarCursos()\" style=\"margin-right:10px\">Siguiente</v-btn>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      cursos_titulo: \"\",\r\n      cursos_tipo: \"\",\r\n      cursos_fecha: \"\",\r\n      cursos_medio: \"\",\r\n      el: \"#example\",\r\n      c: 1,\r\n      cursos: [\r\n        \"Curso\",\r\n        \"Diplomado\",\r\n        \"Seminario\",\r\n        \"Certificación\",\r\n        \"Clínica\",\r\n        \"Congreso\",\r\n        \"Conferencia\"\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    agregar() {\r\n      this.c = this.c + 1;\r\n    },\r\n    eliminar() {\r\n      this.c = this.c - 1;\r\n    },\r\n    agregarCursos() {\r\n      var cursos = {\r\n        cursos_titulo: this.cursos_titulo,\r\n        cursos_tipo: this.cursos_tipo,\r\n        cursos_fecha: this.cursos_fecha,\r\n        cursos_medio: this.cursos_medio \r\n      };\r\n      this.$store.dispatch(\"agregarCursos\", cursos);\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components/formularios"}]}